{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl":".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*":["src/layouts/*"],
      "@configs/*":["./.astropod/*"],
      "@icons/*":["src/components/icons/*"],
      "@content/*":["src/content/*"]
    }
  }
}
